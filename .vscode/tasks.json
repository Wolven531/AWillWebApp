{
	"version": "2.0.0",
	"tasks": [
		{
			"label": "build",
			"command": "dotnet",
			"type": "process",
			"args": [
				"build",
				"${workspaceFolder}/AWillWebApp/AWillWebApp.csproj"
			],
			"problemMatcher": "$msCompile"
		},
		{
			"label": "Run Dotnet App",
			"command": "dotnet",
			"type": "process",
			"args": [
				"run",
				"-p",
				"${workspaceFolder}/AWillWebApp/AWillWebApp.csproj"
			],
			"problemMatcher": "$msCompile"
		}
	]
}
